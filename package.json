{
  "name": "mern",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "build-big-calendar-css": "lessc --autoprefix=\"ie >= 10, last 2 versions\" src/components/BigCalendar/styles.less ./node_modules/react-big-calendar/lib/css/react-big-calendar.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "antd": "^3.19.2",
    "axios": "^0.21.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "3",
    "cloudinary-react": "^1.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.9.0",
    "less-vars-to-js": "^1.3.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "mongoose": "^5.5.11",
    "nodemailer": "^6.1.1",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-big-calendar": "^0.21.0",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.6",
    "react-no-ssr": "^1.1.0",
    "reactjs-layout-slot": "^0.0.7",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Ellyshachavez/DoorsOpen.git"
  }
}
